# Start iDevAffiliate SEO Code

RewriteEngine On

#Custom redirect
RewriteRule ^spread-the-love$ http://www.mytinysecrets\.com/sharethelove [R=301,L]

# Forced Tier Linking Code
RewriteRule ^signup-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/signup\.php?ref=$1 [R=301,L]

# Permanent URL redirect - generated by www.rapidtables.com
Redirect 301 /why-you-shouldnt-give-a-fuck-about-looks/ http://mytinysecrets.com/why-you-should-not-give-a-fuck-about-looks/

# TextAds
RewriteRule ^([0-9]+)-([a-z]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevads\.php?id=$1&ad=$3 [R=301,L]
RewriteRule ^([0-9]+)-([a-z]+)-([0-9]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevads\.php?id=$1&ad=$3&page=$4 [R=301,L]

# Standard Links
RewriteRule ^([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1&page=$2 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1&page=$2&set=$3 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1&page=$2&set=$3&link=$4 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1&page=$2&set=$3&link=$4&keyword=$5 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1&page=$2&set=$3&link=$4&keyword=$5&custom=$6 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)-([0-9]+)-([0-9]+)\.html$ http://mytinysecrets\.com/sharethelove/idevaffiliate\.php?id=$1&page=$2&set=$3&link=$4&keyword=$5&custom=$6&url=$7 [R=301,L]

# End iDevAffiliate SEO Code

RewriteRule ^wiredtree http://wiredtree.com [NC,L,QSA]

# iOS icon

RewriteRule apple-touch-icon.png wp-content/themes/twentyfourteen/images/apple-touch-icon.png [L]
# BEGIN xmlrpc security

<Files xmlrpc.php>
Order Deny,Allow
Deny from all
</Files>

# END xmlrpc security

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress
